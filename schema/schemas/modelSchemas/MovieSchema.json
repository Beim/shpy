{
    "primary_label": "Movie",
    "extends": "",
    "entity_schema": {
        "type": "object",
        "properties": {
            "primary_label": {
                "type": "string",
                "pattern": "^Movie$"
            },
            "property": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "attr_type": "text"
                    },
                    "language": {
                        "type": "string",
                        "attr_type": "text"
                    },
                    "place": {
                        "type": "string",
                        "attr_type": "text"
                    },
                    "description": {
                        "type": "string",
                        "attr_type": "text"
                    },
                    "douban_id": {
                        "type": "string",
                        "attr_type": "categorical"
                    },
                    "data_from": {
                        "type": "string",
                        "attr_type": "text"
                    },
                    "year": {
                        "type": "string",
                        "attr_type": "categorical"
                    },
                    "original_name": {
                        "type": "string",
                        "attr_type": "text"
                    }
                },
                "required": [
                    "name"
                ]
            },
            "related": {
                "type": "object"
            },
            "related_from": {
                "type": "object"
            },
            "related_to": {
                "type": "object",
                "properties": {
                    "has_actor": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "primary_label": {
                                    "type": "string",
                                    "pattern": "^Actor$"
                                }
                            },
                            "required": ["primary_label"]
                        }
                    },
                    "has_director": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "type": "object",
                                "properties": {
                                    "primary_label": {
                                        "type": "string",
                                        "pattern": "^Director$"
                                    }
                                },
                                "required": ["primary_label"]
                            }
                        }
                    },
                    "has_genre": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "primary_label": {
                                    "type": "string",
                                    "pattern": "^Genre$"
                                }
                            },
                            "required": ["primary_label"]
                        }
                    },
                    "has_resource": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "primary_label": {
                                    "type": "string",
                                    "pattern": "^Resource$"
                                }
                            },
                            "required": ["primary_label"]
                        }
                    }
                }
            }
        },
        "required": ["primary_label", "property"]
    }
}