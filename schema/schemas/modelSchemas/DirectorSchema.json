{
    "primary_label": "Director",
    "extends": "Person",
    "entity_schema": {
        "type": "object",
        "properties": {
            "primary_label": {
                "type": "string",
                "pattern": "^Director$"
            },
            "property": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "douban_id": {
                        "type": "string"
                    },
                    "foreign_name": {
                        "type": "string"
                    },
                    "data_from": {
                        "type": "string"
                    }
                },
                "required": ["name"]
            },
            "related": {
                "type": "object"
            },
            "related_from": {
                "type": "object"
            },
            "related_to": {
                "type": "object"
            }
        },
        "required": ["primary_label", "property"]
    }
}