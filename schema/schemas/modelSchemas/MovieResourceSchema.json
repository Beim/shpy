{
    "primary_label": "MovieResource",
    "extends": "Resource",
    "entity_schema": {
        "type": "object",
        "properties": {
            "primary_label": {
                "type": "string",
                "pattern": "^MovieResource$"
            },
            "property": {
                "type": "object",
                "properties": {
                    "url": {
                        "type": "string",
                        "attr_type": "text"
                    },
                    "platform": {
                        "type": "string",
                        "attr_type": "text"
                    },
                    "price": {
                        "type": "string",
                        "attr_type": "numeric"
                    },
                    "data_from": {
                        "type": "string",
                        "attr_type": "text"
                    }
                },
                "required": ["url"]
            },
            "related": {
                "type": "object"
            },
            "related_from": {
                "type": "object",
                "properties": {
                    "has_resource": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "primary_label": {
                                    "type": "string",
                                    "pattern": "^Movie$"
                                }
                            },
                            "required": ["primary_label"]
                        },
                        "minItems": 1
                    }
                },
                "required": ["hasResource"]
            },
            "related_to": {
                "type": "object"
            }
        },
        "required": ["primary_label", "property", "related_from"]
    }
}